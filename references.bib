@article{sororQualityAssuranceModern2021,
  title = {Quality {{Assurance}} in {{Modern Gynecological HDR-Brachytherapy}} ({{Interventional Radiotherapy}}): {{Clinical Considerations}} and {{Comments}}},
  shorttitle = {Quality {{Assurance}} in {{Modern Gynecological HDR-Brachytherapy}} ({{Interventional Radiotherapy}})},
  author = {Soror, Tamer and Siebert, Frank-Andr{\'e} and Lancellotta, Valentina and Placidi, Elisa and Fionda, Bruno and Tagliaferri, Luca and Kov{\'a}cs, Gy{\"o}rgy},
  year = {2021},
  month = jan,
  journal = {Cancers},
  volume = {13},
  number = {4},
  pages = {912},
  publisher = {{Multidisciplinary Digital Publishing Institute}},
  issn = {2072-6694},
  urldate = {2022-11-15},
  abstract = {The use of brachytherapy (interventional radiotherapy) in the treatment of gynecological cancers is a crucial element in both definitive and adjuvant settings. The recent developments in high-dose rate remote afterloaders, modern applicators, treatment-planning software, image guidance, and dose monitoring systems have led to improvement in the local control rates and in some cases improved the survival rates. The development of these highly advanced and complicated treatment modalities has been accompanied by challenges, which have made the existence of quality assurance protocols a must to ensure the integrity of the treatment process. Quality assurance aims at standardizing the technical and clinical procedures involved in the treatment of patients, which could eventually decrease the source of uncertainties whether technical (source/equipment related) or clinical. This commentary review sheds light (from a clinical point of view) on some potential sources of uncertainties associated with the use of modern brachytherapy in the treatment of gynecological cancers.},
  copyright = {http://creativecommons.org/licenses/by/3.0/},
  langid = {english},
  keywords = {brachytherapy,gynecological cancers,interventional radiotherapy,quality assurance},
  file = {C\:\\Users\\25166122M\\Zotero\\storage\\SS2IHEKL\\Soror et al. - 2021 - Quality Assurance in Modern Gynecological HDR-Brac.pdf;C\:\\Users\\25166122M\\Zotero\\storage\\7BF2LP3I\\912.html}
}

@article{spadeaDeepLearningBased2021,
  title = {Deep Learning Based Synthetic-{{CT}} Generation in Radiotherapy and {{PET}}: {{A}} Review},
  shorttitle = {Deep Learning Based Synthetic-{{CT}} Generation in Radiotherapy and {{PET}}},
  author = {Spadea, Maria Francesca and Maspero, Matteo and Zaffino, Paolo and Seco, Joao},
