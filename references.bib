  pmid = {34407209},
  keywords = {artificial intelligence,convolutional neural networks,deep learning,Deep Learning,{Image Processing, Computer-Assisted},image synthesis,machine learning,Magnetic Resonance Imaging,Positron-Emission Tomography,pseudo-CT,radiotherapy,{Radiotherapy Planning, Computer-Assisted},{Tomography, X-Ray Computed}},
