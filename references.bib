@article{villalbaNewTemplateMRIbased2015,
  title = {A New Template for {{MRI-based Intracavitary}}/{{Interstitial}} Gynecologic Brachytherapy: {{Design}} and Clinical Implementation},
  shorttitle = {A New Template for {{MRI-based Intracavitary}}/{{Interstitial}} Gynecologic Brachytherapy},
